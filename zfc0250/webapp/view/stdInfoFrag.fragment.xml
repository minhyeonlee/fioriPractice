<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
>
    <Panel id="stdInfo" expandable="true" expanded="false" headerText="{sName} Student Info">
        <content>
            <l:VerticalLayout>
                <l:HorizontalLayout>
                    <Label text="Studnet No" width="100px"/>
                    <Input id="stdnoInfo" value="{stdNo}" description="{sName}" editable="false" width="200px"/> 
                </l:HorizontalLayout>
                <l:HorizontalLayout>
                    <Label text="Gender" width="100px"/>
                    <Input id="genderInfo" value="{genderCode}" description="{gender}" editable="false" width="200px"/>
                </l:HorizontalLayout>
            </l:VerticalLayout>
        </content>
    </Panel>
</core:FragmentDefinition>